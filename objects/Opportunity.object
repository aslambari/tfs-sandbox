    <fields>
        <fullName>STZebra__ZebraColor__c</fullName>
        <deprecated>false</deprecated>
        <description>Display Zebra Color Based on Zebra Scores.</description>
        <externalId>false</externalId>
        <formula>IF(( STZebra__Zebra_Scores__c &gt;= 0 &amp;&amp; STZebra__Zebra_Scores__c &lt;= 16), &apos;1-Red&apos;,
IF(( STZebra__Zebra_Scores__c &gt;= 17 &amp;&amp; STZebra__Zebra_Scores__c &lt;= 22), &apos;2-Yellow&apos;,
IF(( STZebra__Zebra_Scores__c &gt;= 23 &amp;&amp; STZebra__Zebra_Scores__c &lt;= 28), &apos;3-Green&apos;, &apos;&apos;) ) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Display Zebra Color Based on Zebra Scores.</inlineHelpText>
        <label>ZebraColor</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
